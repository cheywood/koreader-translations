#: frontend/ui/data/koptoptions.lua:142
msgid "Vertical overlap"
msgstr "垂直重叠"

#: frontend/ui/data/koptoptions.lua:160
msgid "Set vertical zoom overlap (between lines)."
msgstr "设置垂直缩放重叠 (行之间)。"

